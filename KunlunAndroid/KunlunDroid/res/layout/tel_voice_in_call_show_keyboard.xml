<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/navpage" >

    <include
        android:id="@+id/relati_top"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/cm_top_bar" />

    <include
        android:id="@+id/relati_keyboard"
        android:layout_width="match_parent"
        android:layout_height="200dip"
        android:layout_below="@id/relati_top"
        android:layout_marginLeft="8dip"
        android:layout_marginRight="8dip"
        android:layout_marginTop="12dip"
        layout="@layout/telephone_dial_keyboard" />

    <include
        android:id="@+id/relati_tel_do_operation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@id/relati_keyboard"
        android:layout_alignRight="@id/relati_keyboard"
        android:layout_below="@id/relati_keyboard"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="15dip"
        layout="@layout/cm_tel_do_operation" />

</RelativeLayout>